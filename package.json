{
  "name": "profile",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "db": "mongod",
    "dev": "ncp ./.tools/.env.development .env && rimraf ./build && razzle start",
    "debug": "ncp ./.tools/.env.development .env && razzle start  --inspect-brk",
    "build": "rimraf ./build && razzle build",
    "coverage": "cross-env NODE_ENV=test nyc mocha --require ./mocha-register.js --timeout 10000 \"./src/**/*.spec.js\"",
    "test": "mocha --require ./mocha-register.js --timeout 10000 ./src/**/*.spec.js",
    "test:watch": "mocha --require ./mocha-register.js --timeout 10000 --watch  ./src/**/*.spec.js",
    "start": "cross-env NODE_ENV=production node build/server.js",
    "deploy": ".tools/publish.sh production",
    "deploy:beta": ".tools/publish.sh beta",
    "tunnel": "ngrok http 3000"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "apicache": "^1.5.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "cross-fetch": "^3.0.0",
    "express": "^4.16.3",
    "feed2json": "^0.6.0",
    "form-data": "^2.3.3",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "pcmli.umbrella.uni-core": "^1.11.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-image-lightbox": "^5.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "reactstrap": "^8.4.1",
    "recompose": "^0.30.0",
    "request": "^2.88.0",
    "serialize-javascript": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@storybook/addons": "^3.4.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "ncp": "^2.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "raf": "^3.4.0",
    "razzle": "^3.0.0",
    "testdouble": "^3.12.5",
    "mocha": "^7.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "dotenv": "6.0.0"
  }
}
