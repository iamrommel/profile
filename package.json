{
  "name": "profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db": "mongod",
    "dev": "ncp ./.tools/.env.development .env && razzle start",
    "debug": "ncp ./.tools/.env.development .env && razzle start  --inspect-brk",
    "build": "PORT=80 NODE_ENV=production razzle build",
    "test": "mocha --require ./mocha-register.js  \"./src/**/*.spec.js\"",
    "test:watch": "mocha --require ./mocha-register.js --watch  ./src/**/*.spec.js",
    "start": "node build/server.js",
    "deploy": "ncp ./.tools/.env.production .env && publish.sh",
    "deploy:beta": "ncp ./.tools/.env.beta .env && publish.sh",
    "tunnel": "ngrok http 3000"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "bootstrap": "^4.1.3",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1"
  },
  "devDependencies": {
    "@storybook/addons": "^3.4.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.5.1",
    "babel-preset-env": "^1.6.1",
    "captainduckduck": "^1.0.15",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-subset": "^1.6.0",
    "copy-webpack-plugin": "^4.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "exports-loader": "^0.7.0",
    "husky": "^1.0.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.3.0",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "raf": "^3.4.0",
    "razzle": "^2.4.0",
    "react-addons-test-utils": "^15.6.2",
    "testdouble": "^3.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
