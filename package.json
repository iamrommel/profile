{
  "name": "profile",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "dev": "ncp ./.tools/.env.development .env && razzle start",
    "debug": "ncp ./.tools/.env.development .env && razzle start  --inspect-brk",
    "build": "PORT=80 NODE_ENV=production razzle build",
    "test": "mocha --require ./mocha-register.js  \"./src/**/*.spec.js\"",
    "test:watch": "mocha --require ./mocha-register.js --watch  ./src/**/*.spec.js",
    "start": "node build/server.js",
    "deploy": "publish.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "express": "^4.16.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "captainduckduck": "^1.0.15",
    "ncp": "^2.0.0",
    "raf": "^3.4.0",
    "razzle": "^2.4.0"
  }
}
